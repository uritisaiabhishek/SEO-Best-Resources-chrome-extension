{
    "manifest_version": 3,
    "name": "SEO Best Resources",
    "version": "1.0",
    "description": "A Chrome extension to access SEO resources categorized by type.",
    "action": {
      "default_popup": "popup.html",
      "default_title": "SEO Resources"
    },
    "permissions": ["tabs"],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  